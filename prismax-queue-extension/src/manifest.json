{
  "manifest_version": 3,
  "name": "PrismaX Queue Notifier",
  "version": "0.1.0",
  "description": "Notifies you when your turn is coming up on prismaX tele-op robotic arms.",
  "permissions": [
    "notifications",
    "storage",
    "debugger",
    "tabs"
  ],
  "host_permissions": [
    "https://app.prismax.ai/*",
    "https://discord.com/api/*",
    "https://discordapp.com/api/*",
    "https://api.telegram.org/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "PrismaX Queue Notifier",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "pxlogo (1).png",
      "32": "pxlogo (2).png",
      "48": "pxlogo (3).png",
      "128": "pxlogo (4).png"
    }
  }
}